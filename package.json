{
  "name": "@cognite/3d-web-parser",
  "version": "0.12.4",
  "description": "Parser for Cognite 3D viewer",
  "contributors": [
    "Anders Hafreager <anders.hafreager@cognite.com>",
    "Fredrik Anfinsen <fredrik.anfinsen@cognite.com>"
  ],
  "author": "Anders Hafreager (anders.hafreager@cognite.com)",
  "license": "Apache-2.0",
  "keywords": [
    "cognite",
    "THREE",
    "3d",
    "parser",
    "typescript"
  ],
  "files": [
      "dist",
      "dist-module"
  ],
  "module": "dist-module/index.js",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm run clean && tsc && webpack -c",
    "buildold": "npm run clean && webpack -c && npm run esCheck",
    "prepush": "npm run lint && npm run test",
    "esCheck": "es-check es5 './dist/index.js'",
    "prepare": "npm run build",
    "serve": "webpack-dev-server"
  },
  "devDependencies": {
    "@cognite/tslint-config-cognite-3d": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.12",
    "@types/three": "^0.93.28",
    "@wasm-tool/wasm-pack-plugin": "1.0.1",
    "es-check": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rollup": "^1.1.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript2": "^0.19.0",
    "text-encoding": "^0.7.0",
    "three": "^0.98.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "protobufjs": "^6.8.8",
    "three": ">=0.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cognitedata/3d-web-parser.git"
  },
  "sideEffects": false,
  "dependencies": {
    "protobufjs": "^6.8.8"
  }
}
